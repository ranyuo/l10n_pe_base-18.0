<odoo>

    <record  id="sale_order_template_view_form" model="ir.ui.view">
        <field name="name">sale_order_template_view_form</field>
        <field name="model">sale.order.template</field>
        <field name="inherit_id" ref="sale_management.sale_order_template_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='sale_order_template_line_ids']/list/field[@name='product_uom_id']" position="after">
                <field name="price_unit"/>
                <field name="discount"/>
                <field name="discount_amount"/>
                <field name="tax_id" widget="many2many_tags"/>
            </xpath>
        </field>
    </record>

</odoo>