<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
        <record id="view_account_payment_register_form_extra_fields_inherit" model="ir.ui.view">
            <field name="name">account.payment.register.form.extra.fields.inherit</field>
            <field name="model">account.payment.register</field>
            <field name="inherit_id" ref="account.view_account_payment_register_form"/>
            <field name="arch" type="xml">


                <field name="partner_bank_id" position="after">

                    <field name="operation_number"/>
                    <field name="sunat_table_01_id" options="{'no_create': True}"/>
                    
                </field>


                <field name="communication" position="after">

                    <field name="l10n_pe_prefix_code"/>
                    <field name="l10n_pe_invoice_number"/>
                    
                </field>

            </field>
        </record>
    </data>

</odoo>
